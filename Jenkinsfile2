def gv
pipeline {
    agent any
    tools{
        maven 'maven-3.9.4'
    }
    stages {
        stage("test") {
            steps {
                script {
                    echo "testing jar"
                }
            }
        }
        stage("build jar") {
            steps {
                script {
                    echo "building jar"
                }
            }
        }
        stage("build image") {
            steps {
                script {
                    echo "building image"
                }
            }
        }
        stage("deploy") {
            steps {
                script {
                    echo "deploying"
                }
            }
        }
    }
}